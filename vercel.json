{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/send-email.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.js"
    }
  ],
  "rewrites": [
    { "source": "/about-us", "destination": "/about-us.html" },
    { "source": "/meet-the-team", "destination": "/meet-the-team.html" },
    { "source": "/testimonials", "destination": "/testimonials.html" },
    { "source": "/gallery", "destination": "/gallery.html" },
    { "source": "/services", "destination": "/services.html" },
    { "source": "/faq", "destination": "/faq.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/", "destination": "/index.html" }
  ]
} 